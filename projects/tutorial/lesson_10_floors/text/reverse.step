step: reverse
    belongs to: text
    expects: input
    returns: result
    
    declare:
        result as text
        index as number
        len as number
    
    do:
        set result to ""
        set len to length of input
        set index to len - 1
        
        while index is greater than or equal to 0
            set result to result added to input[index]
            set index to index - 1
        
        return result

